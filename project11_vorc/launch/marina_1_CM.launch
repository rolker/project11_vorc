<?xml version="1.0"?>
<launch>
  <!-- Gazebo world to load -->
  <arg name="world" default="$(find project11_vorc)/worlds/marina_1_CM.world" />
  <!-- If true, run gazebo GUI -->
  <arg name="gui" default="true" />
  <!-- Do we lock the vessel to the world? -->
  <arg name="robot_locked" default="false" />
  <include file="$(find vorc_gazebo)/launch/marina.launch">
    <arg name="world"   value="$(arg world)"/>
    <arg name="gui"     value="$(arg gui)" />
    <arg name="robot_locked"     value="$(arg robot_locked)" />
  </include>
</launch>

